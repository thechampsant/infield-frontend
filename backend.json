    "info": {
        "title": "Infield Core API",
        "description": "Admin, Account & Project Configuration Platform",
        "version": "1.0",
        "contact": {}
    },
    "tags": [],
    "servers": [],
    "components": {
        "securitySchemes": {
            "bearer": {
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "type": "http"
            },
            "cookie": {
                "type": "apiKey",
                "in": "cookie",
                "name": "Authentication"
            }
        },
        "schemas": {
            "LoginDto": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "example": "superadmin@whynest.com",
                        "description": "User email"
                    },
                    "password": {
                        "type": "string",
                        "example": "SuperAdmin@123",
                        "description": "User password"
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            },
            "LoginResponseDto": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "description": "JWT access token"
                    },
                    "user": {
                        "type": "object",
                        "description": "User information"
                    }
                },
                "required": [
                    "accessToken",
                    "user"
                ]
            },
            "CreateAccountDto": {
                "type": "object",
                "properties": {
                    "accountName": {
                        "type": "string",
                        "description": "Display name of the account",
                        "example": "Acme Corporation"
                    },
                    "email": {
                        "type": "string",
                        "description": "Primary contact email for the account",
                        "example": "admin@acme.com"
                    },
                    "status": {
                        "type": "string",
                        "description": "Account status",
                        "enum": [
                            "ACTIVE",
                            "INACTIVE"
                        ],
                        "default": "ACTIVE"
                    }
                },
                "required": [
                    "accountName",
                    "email"
                ]
            },
            "UpdateAccountDto": {
                "type": "object",
                "properties": {
                    "accountName": {
                        "type": "string",
                        "description": "Display name of the account",
                        "example": "Acme Corporation Updated"
                    },
                    "email": {
                        "type": "string",
                        "description": "Primary contact email for the account",
                        "example": "newadmin@acme.com"
                    },
                    "status": {
                        "type": "string",
                        "description": "Account status",
                        "enum": [
                            "ACTIVE",
                            "INACTIVE"
                        ]
                    }
                }
            },
            "CreateProjectDto": {
                "type": "object",
                "properties": {
                    "accountId": {
                        "type": "string",
                        "description": "ID of the parent account",
                        "example": "507f1f77bcf86cd799439011"
                    },
                    "projectName": {
                        "type": "string",
                        "description": "Display name of the project",
                        "example": "Mumbai Operations"
                    },
                    "email": {
                        "type": "string",
                        "description": "Primary contact email for the project",
                        "example": "mumbai@acme.com"
                    },
                    "status": {
                        "type": "string",
                        "description": "Project status",
                        "enum": [
                            "ACTIVE",
                            "INACTIVE"
                        ],
                        "default": "ACTIVE"
                    }
                },
                "required": [
                    "accountId",
                    "projectName",
                    "email"
                ]
            },
            "UpdateProjectDto": {
                "type": "object",
                "properties": {
                    "projectName": {
                        "type": "string",
                        "description": "Display name of the project",
                        "example": "Mumbai Operations Updated"
                    },
                    "email": {
                        "type": "string",
                        "description": "Primary contact email for the project",
                        "example": "newmumbai@acme.com"
                    },
                    "status": {
                        "type": "string",
                        "description": "Project status",
                        "enum": [
                            "ACTIVE",
                            "INACTIVE"
                        ]
                    }
                }
            }
        }
    }
